<Ui xmlns="http://www.blizzard.com/wow/ui/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">

	<Frame name="AegisFrame">
		<Scripts>
			<OnLoad>
				aegis.onload()
			</OnLoad>
			<OnUpdate>
				aegis.onupdate()
			</OnUpdate>
			<OnEvent>
				aegis.onevent()
			</OnEvent>
		</Scripts>
	</Frame>
	<GameTooltip name="Aegis_Tooltip" frameStrata="TOOLTIP" hidden="true" parent="UIParent" inherits="GameTooltipTemplate">
		<Scripts>
			<OnLoad>
		this:SetOwner(WorldFrame, "ANCHOR_NONE");
			</OnLoad>
		</Scripts>
	</GameTooltip>
</Ui>